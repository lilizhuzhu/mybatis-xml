<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.demo.mapper.test1.CommonMapper">


    <select id="sqlQueryByCondition" resultType="map" parameterType="org.example.demo.common.SqlQueryRequest">
        ${sql}
        limit #{pageStart}, #{pageSize}
    </select>
    <select id="queryList" resultType="java.util.Map">
        select * from people
        WHERE  address = "蜀国"
    </select>
</mapper>